{
  "manifest_version": 3,
  "name": "SNA Labs Theme Toggle",
  "version": "1.0",
  "description": "Toggle dark theme for SNA Labs HTML pages",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "dark-icon.png"
  },
  "icons": {
    "128": "dark-icon.png"
  }
} 